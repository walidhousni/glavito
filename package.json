{
  "name": "@glavito-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:admin\"",
    "dev:mobile": "nx start @glavito-workspace/mobile",
    "dev:mobile:web": "nx serve @glavito-workspace/mobile",
    "dev:mobile:ios": "nx run @glavito-workspace/mobile:run-ios -- --device \"iPhone 16 Pro\"",
    "dev:mobile:android": "nx run @glavito-workspace/mobile:run-android",
    "dev:api": "nx serve api-gateway",
    "dev:admin": "nx dev admin-dashboard --port=3004",
    "kill:dev": "bash scripts/kill-dev.sh",
    "tunnel:both": "ngrok start --config ops/tunnels/ngrok.yml api-gateway admin-dashboard",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:admin\" \"npm run tunnel:both\"",
    "dev:all:public": "concurrently \"nx serve api-gateway\" \"nx dev admin-dashboard --port=3004\" \"ngrok start --config ops/tunnels/ngrok.yml api-gateway admin-dashboard\"",
    "dev:docker": "docker-compose up --build",
    "dev:docker:detached": "docker-compose up --build -d",
    "build": "nx build api-gateway && nx build admin-dashboard && nx build @glavito-workspace/mobile",
    "build:all": "nx run-many --target=build --all",
    "test": "nx test api-gateway && nx test @glavito-workspace/mobile",
    "perf:smoke": "artillery run ./.kiro/perf/smoke.yml",
    "perf:baseline": "artillery run ./.kiro/perf/baseline.yml",
    "test:all": "nx run-many --target=test --all",
    "lint": "nx lint api-gateway && nx lint @glavito-workspace/mobile",
    "lint:all": "nx run-many --target=lint --all",
    "typecheck": "nx typecheck api-gateway",
    "typecheck:all": "nx run-many --target=typecheck --all",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "db:clean": "node scripts/clean-database.js",
    "db:studio": "prisma studio",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:fresh": "npm run db:clean && npm run db:seed",
    "dev:setup": "node scripts/setup-dev.js",
    "dev:verify": "node scripts/verify-setup.js",
    "n8n:setup": "node scripts/setup-n8n.js",
    "n8n:test": "node scripts/test-n8n-integration.js",
    "n8n:start": "docker-compose up -d n8n",
    "n8n:stop": "docker-compose stop n8n",
    "n8n:logs": "docker-compose logs -f n8n",
    "start": "npm run dev:docker",
    "clean": "rm -rf dist node_modules/.cache",
    "fresh": "npm run docker:clean && npm run setup && npm run dev:docker",
    "build:images": "docker build -f api-gateway/Dockerfile -t api:local . && docker build -f apps/admin-dashboard/Dockerfile -t admin:local .",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
