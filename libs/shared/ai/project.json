{
  "name": "shared-ai",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/ai/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/ai",
        "main": "libs/shared/ai/index.ts",
        "tsConfig": "libs/shared/ai/tsconfig.lib.json",
        "assets": [
          {
            "input": "libs/shared/ai",
            "glob": "*.md",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/ai/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/ai/jest.config.ts"
      }
    }
  },
  "tags": ["scope:shared", "type:library"]
}